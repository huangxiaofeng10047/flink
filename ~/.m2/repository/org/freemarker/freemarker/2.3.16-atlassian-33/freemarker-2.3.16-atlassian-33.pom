<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd ">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.freemarker</groupId>
  <artifactId>freemarker</artifactId>
  <version>2.3.16-atlassian-33</version>
  <packaging>jar</packaging>

  <name>Freemarker</name>
  <description>FreeMarker is a "template engine"; a generic tool to generate text output based on templates.</description>
  <url>http://freemarker.org/</url>

  <scm>
    <connection>scm:svn:https://svn.atlassian.com/svn/public/atlassian/vendor/freemarker-2.3.16/tags/freemarker-2.3.16-atlassian-33</connection>
    <developerConnection>scm:svn:https://svn.atlassian.com/svn/public/atlassian/vendor/freemarker-2.3.16/tags/freemarker-2.3.16-atlassian-33</developerConnection>
    <url>https://svn.atlassian.com/svn/public/atlassian/vendor/freemarker-2.3.16/tags/freemarker-2.3.16-atlassian-33</url>
  </scm>

  <licenses>
    <license>
      <name>BSD-style license</name>
      <url>http://freemarker.org/docs/app_license.html</url>
    </license>
  </licenses>
  <properties>
    <skipTests>true</skipTests>
  </properties>

  <build>
    <sourceDirectory>src</sourceDirectory>
    <resources>
      <resource>
        <directory>src</directory>
        <includes>
          <include />
        </includes>
      </resource>
    </resources>
    <testSourceDirectory>src</testSourceDirectory>
    <testResources>
      <testResource>
        <directory>src</directory>
        <includes>
          <include>freemarker/testcase/reference/**</include>
          <include>freemarker/testcase/servlets/reference/**</include>
          <include>freemarker/testcase/servlets/template/**</include>
          <include>freemarker/testcase/template/**</include>
          <include>freemarker/testcase/servlets/WEB-INF/**</include>
          <include>freemarker/testcase/*.properties</include>
          <include>freemarker/testcase/*.xml</include>
          <include>freemarker/testcase/models/*.properties</include>
        </includes>
      </testResource>
    </testResources>

    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
          <includes>
            <include>freemarker/cache/**</include>
            <include>freemarker/core/**</include>
            <include>freemarker/debug/**</include>
            <include>freemarker/ext/**</include>
            <include>freemarker/log/**</include>
            <include>freemarker/maps/**</include>
            <include>freemarker/template/**</include>
          </includes>
          <excludes>
            <exclude>freemarker/ext/jsp/FreeMarkerPageContext1.java</exclude> <!-- OK -->
            <exclude>freemarker/ext/jsp/FreeMarkerPageContext2.java</exclude>
            <exclude>freemarker/ext/jsp/FreeMarkerJspFactory2.java</exclude>
          </excludes>
          <testIncludes>
            <include>freemarker/testcase/**</include>
          </testIncludes>
        </configuration>
      </plugin>
      <!--<plugin>-->
        <!--<artifactId>maven-surefire-plugin</artifactId>-->
        <!--<configuration>-->
          <!--<includes>-->
            <!--<include>freemarker/testcase/**</include>-->
          <!--</includes>-->
        <!--</configuration>-->
      <!--</plugin>-->

      <plugin>
      <artifactId>exec-maven-plugin</artifactId>
      <groupId>org.codehaus.mojo</groupId>
      <executions>
      <execution><!-- Run our version calculation script -->
        <phase>generate-sources</phase>
        <goals>
          <goal>exec</goal>
        </goals>
        <configuration>
          <executable>ant</executable>
        </configuration>
      </execution>
        </executions>

      </plugin>


    </plugins>
  </build>

  <dependencies>
    <dependency>
      <groupId>org.apache.ant</groupId>
      <artifactId>ant</artifactId>
      <version>1.7.0</version>
    </dependency>
    <dependency>
      <groupId>emma</groupId>
      <artifactId>emma</artifactId>
      <version>2.0.4217</version>
    </dependency>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>servlet-api</artifactId>
      <version>2.5</version>
    </dependency>
    <dependency>
      <groupId>javax.servlet.jsp</groupId>
      <artifactId>jsp-api</artifactId>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>jaxen</groupId>
      <artifactId>jaxen</artifactId>
      <version>1.1.1</version>
    </dependency>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>b8</version>
    </dependency>
    <dependency>
      <groupId>jython</groupId>
      <artifactId>jython</artifactId>
      <version>2.1</version>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.16</version>
    </dependency>
    <dependency>
      <groupId>logkit</groupId>
      <artifactId>logkit</artifactId>
      <version>1.2</version>
    </dependency>
    <dependency>
      <groupId>rhino</groupId>
      <artifactId>js</artifactId>
      <version>1.7R2</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.2</version>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>2.5</version>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>atlassian-3rdparty</id>
      <url>https://maven.atlassian.com/3rdparty</url>
    </repository>
  </distributionManagement>
</project>
